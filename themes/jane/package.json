{
  "name": "hugo-theme-jane",
  "version": "1.0.0",
  "description": "A simple theme for hugo",
  "main": "",
  "scripts": {
    "build": "webpack",
    "dev:assets": "webpack --watch --display='errors-only' ",
    "dev:site": "hugo server --buildDrafts --contentDir='./exampleSite/content' ",
